{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "NitroxServer": "Debug"
    }
  },
  "ServerMode": {
    "UseGenericHost": true,
    "EnableAdvancedFeatures": true,
    "EnableAutoFallback": true
  },
